{
    "framework": "nextjs",
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=63072000; includeSubDomains; preload"
                }
            ]
        }
    ],
    "redirects": [],
    "images": {
        "sizes": [
            640,
            750,
            828,
            1080,
            1200,
            1920,
            2048,
            3840
        ],
        "domains": [
            "img.clerk.com"
        ]
    }
}